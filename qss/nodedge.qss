GraphicsNodeContent {
  background: transparent;
}
GraphicsNodeContent QFrame {
  background: transparent;
}
GraphicsNodeContent QTextEdit,
GraphicsNodeContent QLineEdit {
  background: #666;
  color: #fff;
}
GraphicsNodeContent QLabel {
  color: #e0e0e0;
}
GraphicsNodeContent QLabel#BlockBackground {
  background: transparent;
  height: 0px;
  color: #484848;
  font-size: 72px;
  max-height: 49px;
  min-height: 49px;
  padding-left: 94px;
}
GraphicsNodeContent QLabel#BlockBackground {
  padding-top: 12px;
}
GraphicsNodeContent QLabel#OutputBlockContent {
  min-width: 150px;
  max-width: 150px;
  min-height: 45px;
  max-height: 45px;
  margin-left: 10px;
  margin-top: 5px;
  font-size: 28px;
}
GraphicsNodeContent QLineEdit#InputBlockContent {
  background: #505050;
  width: 140px;
  height: 40px;
  margin-top: 5px;
  margin-left: 5px;
  font-size: 28px;
}
QGraphicsView {
  selection-background-color: #00f;
}
QPushButton {
  background-color: #008000;
  color: #fff;
}
QPushButton#danger {
  background: url("icons/_example.png");
  color: #000;
  font-weight: bold;
  color: #f00;
}
